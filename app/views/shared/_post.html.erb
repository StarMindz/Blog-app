<div class="post_container">
  <h2><%= post.title %></h2>
  <div><%= post.text %></div>
  <div class="interaction">
   <div>Number of Comments<%=post.comments_counter%></div>
   <div>Number of Likes:<%=post.likes_counter%></div>
  </div>
  <p><%= link_to "view post",  user_post_path(@user, post)%></p>
</div>